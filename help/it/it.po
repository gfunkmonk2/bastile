#: C/bastile.xml:194(title)
msgid "The <application>Passwords and Encryption Keys</application> Window"
msgstr ""
"La finestra di <application>Password e chiavi di cifratura</application>"

#: C/bastile.xml:202(application)
msgid "Passwords and Encryption Keys"
msgstr "Password e chiavi di cifratura"

#: C/bastile.xml:201(phrase)
msgid "Show the <placeholder-1/> window."
msgstr "Mostra la finestra di <placeholder-1/>."

#: C/bastile.xml:209(para)
msgid ""
"The <application>Passwords and Encryption Keys</application> window contains "
"the following elements:"
