#: ../data/bastile.schemas.in.h:7
msgid "Controls the visibility of the trust column for the key manager."
msgstr "ལྡེ་མིག་འཛིན་སྐྱོང་པའི་དོན་ལས་ བློ་གཏད་ཀེར་ཐིག་གི་མཐོང་གསལ་ ཚད་འཛིན་འབདཝ་ཨིན།"

#: ../data/bastile.schemas.in.h:8
msgid "Controls the visibility of the type column for the key manager."
msgstr "ལྡེ་མིག་འཛིན་སྐྱོང་པའི་དོན་ལས་ དབྱེ་བའི་ཀེར་ཐིག་གི་མཐོང་གསལ་ ཚད་འཛིན་འབདཝ་ཨིན།"

#: ../data/bastile.schemas.in.h:9
msgid "Controls the visibility of the validity column for the key manager."
msgstr "ལྡེ་མིག་འཛིན་སྐྱོང་པའི་དོན་ལས་ བློ་གཏད་ཀེར་ཐིག་གི་མཐོང་གསལ་ ཚད་འཛིན་འབདཝ་ཨིན།"

#: ../data/bastile.schemas.in.h:10
msgid "Display cache reminder in the notification area"
msgstr "བརྡ་བསྐུལ་མངའ་ཁོངས་ནང་ འདྲ་མཛོད་དྲན་བསྐུལ་ བཀྲམ་སྟོན་འབད་"

#: ../data/bastile.schemas.in.h:11
msgid "Display clipboard after decrypting"
msgstr "གསང་བཤོལ་འབད་བའི་ཤུལ་མ་ འཛིན་པང་བཀྲམ་སྟོན་འབད་"

#: ../data/bastile.schemas.in.h:12
msgid "Display clipboard after encrypting"
msgstr "གསང་བཟོའི་ཤུལ་མ་ འཛིན་པང་བཀྲམ་སྟོན་འབད་"

#: ../data/bastile.schemas.in.h:13
msgid "Enable DNS-SD sharing"
msgstr "ཌི་ཨེན་ཨེསི་-ཨེསི་ཌི་རུབ་སྤྱོད་ ལྕོགས་ཅན་བཟོ་"

#: ../data/bastile.schemas.in.h:14
msgid "Enables DNS-SD (Apple Bonjour) sharing of keys. bastile-daemon must running and must be built with HKP and DNS-SD support."
msgstr "ཌི་ཨེན་ཨེསི་-ཨེསི་ཌི་(ཨེ་པཱལ་བཱོན་ཇོར་)ལྡེ་མིག་རུབ་སྤྱོད་ ལྕོགས་ཅན་བཟོཝ་ཨིན། སི་ཧོརསི་-ཌེ་མཱོན་འདི་གཡོག་བཀོལ་ཏེ་བཞག་དགོཔ་དང་ ཨེཅ་ཀེ་པི་དང་ཌི་ཨེན་ཨེསི་-ཨེསི་ཌི་རྒྱབ་སྐྱོར་དང་གཅིག་ཁར་བཟོ་རྩིག་འབད་དགོཔ་ཨིན།"

#: ../data/bastile.schemas.in.h:15
msgid "Expire passwords in the cache"
msgstr "འདྲ་མཛོད་ནང་གི་ དུས་ཡོལ་ཆོག་ཡིག་ཚུ་"

#: ../data/bastile.schemas.in.h:16
msgid "ID of the default key"
msgstr "སྔོན་སྒྲིག་ལྡེ་མིག་གི་ཨའི་ཌི་"

#: ../data/bastile.schemas.in.h:17
msgid "If set to 'mate' uses mate-keyring to cache passwords. When set to 'internal' uses internal cache."
msgstr " 'mate' ལུ་གཞི་སྒྲིག་འབད་དེ་ཡོད་པ་ཅིན་ འདྲ་མཛོད་ཆོག་ཡིག་ལུ་ཇི་ནོམ་-ལྡེ་འཁོར་ལག་ལེན་འཐབ་ཨིན། 'internal' ལུ་གཞི་སྒྲིག་འབད་དེ་ཡོད་པ་ཅིན་ ནང་འཁོད་འདྲ་མཛོད་ལག་ལེན་འཐབ་ཨིན།"

#: ../data/bastile.schemas.in.h:18
msgid "If set to true, then files encrypted with bastile will be ASCII armor encoded."
msgstr "བདེན་པ་སྦེ་གཞི་སྒྲིག་འབད་དེ་ཡོད་པ་ཅིན་ སི་ཧོརསི་དང་བཅས་གསང་བ་བཟོ་ཡོད་པའི་ཡིག་སྣོད་ཚུ་ ཨེ་ཨེསི་སི་ཨའི་ཨའི་ ཨར་མོར་གྱིས་ཨེན་ཀོཌ་འབད་འོང་།"

#: ../data/bastile.schemas.in.h:19
msgid "If set to true, then the default key will always be added to an encryption recipients list."
msgstr "བདེན་སྦེ་གཞི་སྒྲིག་འབད་དེ་ཡོད་པ་ཅིན་ སྔོན་སྒྲིག་ལྡེ་མིག་འདི་ཨ་རྟག་ར་ གསང་བཟོ་ཐོབ་མཁན་ཐོ་ཡིག་ནང་ཁ་སྐོང་བརྐྱབ་འོང་།"

#: ../data/bastile.schemas.in.h:20
msgid "Last key server search pattern"
msgstr "མཇུག་གི་ ཀི་སར་བར་འཚོལ་ཞིབ་དཔེ་གཞི"

#: ../data/bastile.schemas.in.h:21
msgid "Last key servers used"
msgstr "མཇུག་གི་ ཀི་སར་བར་ལག་ལེན་འཐབ་ཡོདཔ"

#: ../data/bastile.schemas.in.h:22
msgid "Last key used to sign a message."
msgstr "འཕྲིན་དོན་ཅིག་མིང་རྟགས་བཀོལ་ནི་གི་དོན་ལས་ མཇུག་གི་ལྡེ་མིག་ལག་ལེན་འཐབ་ནུག"

#: ../data/bastile.schemas.in.h:23
msgid "PGP Key servers"
msgstr "པི་ཇི་པི་ ལྡེ་མིག་སར་བར་ཚུ་"

#: ../data/bastile.schemas.in.h:24
msgid "Prompt before using GPG passwords in cache"
msgstr "འདྲ་མཛོད་ནང་ལུ་ ཇི་པི་ཇི་ཆོག་ཡིག་ཚུ་ལག་ལེན་མ་འཐབ་པའི་ཧེ་མ་ སྣུས་པ་སྤེལ་དགོ"

#: ../data/bastile.schemas.in.h:25
msgid "Publish keys to this key server."
msgstr "ཀི་སར་བར་ལུ་ ལྡེ་མིག་དཔེ་བསྐྲུན་འབད།"

#: ../data/bastile.schemas.in.h:26
msgid "Reflect the contents of the clipboard (whether encrypted, signed, etc...) in the panel applet icon."
msgstr "པེ་ནཱལ་ཨེཔ་ལེཊི་ངོས་པར་ནང་ལུ་ འཛིན་པང་(གསང་བཟོས་ མིང་རྟགས་བཀོད་ཡོདཔ་ལ་སོགས་པ་ག་ཅི་ཨིན་རུང་)གི་ནང་དོན་ཚུ་ འཆར་སྣང་འབྱུངམ་ཨིན།"

#: ../data/bastile.schemas.in.h:27
msgid "Set to 'false' to disable display of the cache reminder in the notification area of your panel."
msgstr "ཁྱོད་རའི་པེ་ནཱལ་གྱི་བརྡ་བསྐུལ་མངའ་ཁོངས་ནང་ འདྲ་མཛོད་དྲན་བསྐུལ་གྱི་བཀྲམ་སྟོན་ ལྕོགས་མིན་བཟོ་ཐབས་ལུ་ 'false' ལུ་གཞི་སྒྲིག་འབད།"

#: ../data/bastile.schemas.in.h:28
msgid "Set to 'true' to have bastile-agent prompt before giving out passwords it has cached."
msgstr "འདྲ་མཛོད་ནང་བཙུགས་ཡོད་པའི་ཆོག་ཡིག་ཚུ་ མ་བྱིན་པའི་ཧེ་མ་ སི་ཧོརསི་-ལས་ཚབ་འདི་ནུས་སྤེལ་གྱི་དོན་ལུ་ 'true' ལུ་གཞི་སྒྲིག་འབ་ད།"

#: ../data/bastile.schemas.in.h:29
msgid "Show clipboard state in panel"
msgstr "པེ་ནཱལ་ནང་ལུ་ འཛིན་པང་གནས་ལུགས་སྟོན་"

#: ../data/bastile.schemas.in.h:30
msgid "Show expires column in key manager"
msgstr "ལྡེ་མིག་འཛིན་སྐྱོང་པའི་ནང་ དུས་ཡོལ་ཀེར་ཐིག་སྟོན་"

#: ../data/bastile.schemas.in.h:31
msgid "Show trust column in key manager"
msgstr "ལྡེ་མིག་འཛིན་སྐྱོང་པའི་ནང་ བློ་གཏད་ཀེར་ཐིག་སྟོན་"

#: ../data/bastile.schemas.in.h:32
msgid "Show type column in key manager"
msgstr "ལྡེ་མིག་འཛིན་སྐྱོང་པའི་ནང་ དབྱེ་བའི་ཀེར་ཐིག་སྟོན་"

#: ../data/bastile.schemas.in.h:33
msgid "Show validity column in key manager"
msgstr "ལྡེ་མིག་འཛིན་སྐྱོང་པའི་ནང་ ནུས་ལྡན་དུས་ཚོད་ཀྱི་ཀེར་ཐིག་སྟོན་"

#: ../data/bastile.schemas.in.h:34
msgid "Specify the column to sort the recipients window by. Columns are: 'name' and 'id'. Put a '-' in front of the column name to sort in descending order."
msgstr "ཐོབ་མཁན་གྱི་སྒོ་སྒྲིག་དབྱེ་སེལ་འབད་ནི་གི་དོན་ལས་ ཀེར་ཐིག་གསལ་བཀོད་འབད། ཀེར་ཐིག་ཚུ་: 'name' and 'id' ཚུ་ཨིན། མར་འབབ་གོ་རིམ་གྱི་ཐོག་ལས་དབྱེ་སེལ་འབད་ནི་ལུ་  '-' འདི་ ཀེར་ཐིག་མིང་གི་གདོང་ཁ་བཙུགས།"

#: ../data/bastile.schemas.in.h:35
msgid "Specify the column to sort the bastile key manager main window by. Columns are: 'name', 'id', 'validity', 'expires', 'trust', and 'type'. Put a '-' in front of the column name to sort in descending order."
msgstr "སི་ཧོརསི་ལྡེ་མིག་འཛིན་སྐྱོང་པའི་སྒོ་སྒྲིག་སྒོ་སྒྲིག་ངོ་མ་དབྱེ་སེལ་འབད་ནི་གི་དོན་ལུ་ ཀེར་ཐིག་གསལ་བཀོད་འབད། ཀེར་ཐིག་ཚུ་: 'name', 'id', 'validity', 'expires', 'trust', དང་ 'type'ཚུ་ཨིན། '-' འདི་ མར་འབབ་གོ་རིམ་གྱི་ཐོག་ལས་དབྱེ་སེལ་འབད་ནི་ལུ་ ཀེར་ཐིག་མིང་གི་གདོང་ཁར་བཙུགས།"

#: ../data/bastile.schemas.in.h:36
msgid "The ID of the last secret key used to sign a message."
msgstr "མཇུག་གི་གསང་ལྡེའི་ཨའི་ཌི་འདི་ འཕྲིན་དོན་ཅིག་མིང་རྟགས་བཀོད་ནི་གི་དོན་ལུ་ ལག་ལེན་འཐབ་ཡོདཔ།"

#: ../data/bastile.schemas.in.h:37
msgid "The column to sort the recipients by"
msgstr "ཐོབ་མཁན་ཚུ་དབྱེ་སེལ་འབད་ནིའི་ཀེར་ཐིག་"

#: ../data/bastile.schemas.in.h:38
msgid "The column to sort the bastile keys by"
msgstr "སི་ཧོརསི་ལྡེ་མིག་དབྱེ་སེལ་འབད་ནིའི་ཀེར་ཐིག་"

#: ../data/bastile.schemas.in.h:39
msgid "The key server to publish PGP keys to. Or empty to suppress publishing of PGP keys."
msgstr "པི་ཇི་པི་ལྡེ་མིག་དཔེ་བསྐྲུན་འབད་ནིའི་ལྡེ་འཁོར། ཡང་ན་ པི་ཇི་པི་ལྡེ་མིག་དཔེ་བསྐྲུན་འབད་ནི་ལས་མར་མནོན་འབད་ནི་གི་དོན་ལུ་སྟོངམ་སྟོནམ་ཨིན།"

#: ../data/bastile.schemas.in.h:40
msgid "The last key server a search was performed against or empty for all key servers."
msgstr "ཀི་སར་་བརསི་ཆ་མཉམ་གྱི་དོན་ལུ་སྟོངམ་ ཡང་ན་ རྒྱབ་འགལ་དུ་མཇུག་གི་ཀི་སར་་བར་འཚོལ་ཞིབ་ཅིག་ ལས་འགན་གྲུབ་ཡོདཔ།"

#: ../data/bastile.schemas.in.h:41
msgid "The last search pattern searched for against a key server."
msgstr "ཀི་སར་་བར་ཅིག་གི་རྒྱབ་འགལ་སྦེ་ མཇུག་གི་འཚོལ་ཞིབ་དཔེ་གཞི་ འཚོལ་ཞིབ་འབད་ཡོདཔ།"

#: ../data/bastile.schemas.in.h:42
msgid "The time (in minutes) to cache GPG passwords"
msgstr "པི་ཇི་པི་ཆོག་ཡིག་ཚུ་ འདྲ་མཛོད་ནང་བཙུག་ནིའི་ཆུ་ཚོད་(དཀར་མ་ནང་)"

#: ../data/bastile.schemas.in.h:43
msgid "This is the amount of time, specified in minutes, to cache GPG passwords in bastile-agent."
msgstr "འ་ནི་འདི་ སི་ཧོརསི་-ལས་ཚབ་ནང་ ཇི་པི་ཇི་ཆོག་ཡིག་ཚུ་འདྲ་མཛོད་ནང་བཙུག་ནིའི་དཀར་ཆ་ནང་གསལ་བཀོད་འབད་ཡོད་པའི་ཆུ་ཚོད་ཀྱི་བསྡོམས་ཨིན།"

#: ../data/bastile.schemas.in.h:44
msgid "This option enables the GPG password cache in the bastile-agent program. The 'use-agent' setting in gpg.conf affects this setting."
